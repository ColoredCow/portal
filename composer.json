{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],

    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/ColoredCow/laravel-samlidp"
        }
    ],


    "license": "MIT",
    "require": {
        "php": "^7.3",
        "aws/aws-sdk-php": "^3.121",
        "barryvdh/laravel-snappy": "^0.4.7",
        "codegreencreative/laravel-samlidp": "dev-master",
        "doctrine/dbal": "^2.10",
        "fideloper/proxy": "^4.0",
        "google/apiclient": "^2.4",
        "h4cc/wkhtmltopdf-amd64": "0.12.x",
        "jgrossi/corcel": "^4.0",
        "jordikroon/google-vision": "^1.8",
        "laravel/framework": "^7.0",
        "laravel/helpers": "^1.1",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.0",
        "niklasravnsborg/laravel-pdf": "^3.1",
        "nwidart/laravel-modules": "^7.0",
        "spatie/laravel-permission": "^3.3"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "facade/ignition": "^2.0",
        "filp/whoops": "^2.5",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^4.1",
        "nunomaduro/larastan": "^0.5.7",
        "phpunit/phpunit": "^8.5"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "Modules/",
            "OfficeSuite\\": "OfficeSuite/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
